{
  "name": "prisma-decapi-class-generator",
  "description": "Prisma generator for generating Decapi class types and enums",
  "version": "0.2.0",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-decapi-class-generator": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "prepack": "yarn build",
    "test": "jest",
    "prepublishOnly": "yarn build",
    "publish-locally": "yalc push",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@prisma/generator-helper": "^4.0.0",
    "chalk": "^4",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "prettier": "^2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.3",
    "@types/prettier": "^2.6.3",
    "jest": "^28.1.2",
    "prisma": "^4.0.0",
    "semantic-release": "^19.0.3",
    "typescript": "^4.7.4"
  },
  "homepage": "https://github.com/capaj/prisma-decapi-class-generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/capaj/prisma-decapi-class-generator.git"
  },
  "contributors": [
    "Yassin Eldeeb <yassineldeeb94@gmail.com>",
    "Jiri Spac <capajj@gmail.com>"
  ],
  "keywords": [
    "prisma",
    "prisma2",
    "decapi",
    "prisma-framework",
    "graphql",
    "generator",
    "copilot",
    "classes",
    "enums",
    "types"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
